{
  "manifest_version": 3,
  "name": "Currency Converter Addon",
  "version": "1.0",
  "description": "Convert currencies between countries.",
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["https://api.exchangerate-api.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}